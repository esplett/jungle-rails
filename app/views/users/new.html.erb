
<header class="page-header">
 <h1>Register</h1>
</header>


<table class="table table-bordered">
    <%= form_for :user, url: 'users' do |f| %>

    <thead>
       
    </thead>
    <tbody>
        <tr>
            <td>First Name:</td><td> <%= f.text_field :first_name %></td>
        </tr>
        <tr>
            <td>Last Name:</td><td><%= f.text_field :last_name %></td>
        </tr>
        <tr>
            <td>Email:</td><td><%= f.text_field :email %></td>
        </tr>
        <tr>
            <td>Password:</td><td><%= f.password_field :password %></td>
        </tr>
        <tr>
            <td>Password Confirmation:</td><td><%= f.password_field :password_confirmation %> </td>
        </tr>
    </tbody>
</table>
<%= f.submit "Submit" %>
    <% end %>

